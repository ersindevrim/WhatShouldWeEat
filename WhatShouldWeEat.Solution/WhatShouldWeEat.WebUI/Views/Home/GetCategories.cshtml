@model IEnumerable<WhatShouldWeEat.Entities.categories>

<div class="container padding-top-100px">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-xl-2 col-lg-3 col-md-4 col-6 sm-mb-25px">
            <a href="#" class="d-block box-shadow background-main-color text-white hvr-float">
                <div class="thum"><img src="~/assets/img/cat-2.jpg" alt=""></div>
                <h4 class="text-center padding-15px">@item.categoryname</h4>
            </a>
        </div>
        }
    </div>
</div>